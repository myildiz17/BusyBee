<% if flash[:errors] %>
    <% flash[:errors].each do |err| %>
        <p style="color: red"><%= err %></p>
    <% end %>
<% end %>

<%= form_for [@neighbor, @child] do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :age %>
    <%= f.text_field :age %>

   
    <%= f.submit %>
    
<% end %>
 