<h1>Sign Up</h1>

   <%= form_tag sessions_path do %>
      <%= label_tag "Username" %>
      <%= text_field_tag :name %>
      <%= label_tag "Address" %>
      <%= text_field_tag :address %>
      <%= label_tag "Password" %>
      <%= password_field_tag :password %>
      <%= submit_tag "Sign Up" %>
<%end%>
